<% if (current_user.voted_for? @eatery) %>
  $(".like-heart").removeClass("fa-heart-o").addClass("fa-heart")
  $("#like-container").addClass("pulse").addClass("animated").addClass("infinite")
  $(".like-count").html(parseInt($(".like-count").html()) + 1)
<% else %>
  $(".like-heart").removeClass("fa-heart").addClass("fa-heart-o")
  $("#like-container").removeClass("pulse").removeClass("animated").removeClass("infinite")
  $(".like-count").html(parseInt($(".like-count").html()) - 1)
<% end %>