<h1>Listing wishpond_tests</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Bio</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wishpond_tests.each do |wishpond_test| %>
      <tr>
        <td><%= wishpond_test.first_name %></td>
        <td><%= wishpond_test.last_name %></td>
        <td><%= wishpond_test.bio %></td>
        <td><%= wishpond_test.email %></td>
        <td><%= link_to 'Show', wishpond_test %></td>
        <td><%= link_to 'Edit', edit_wishpond_test_path(wishpond_test) %></td>
        <td><%= link_to 'Destroy', wishpond_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wishpond test', new_wishpond_test_path %>
